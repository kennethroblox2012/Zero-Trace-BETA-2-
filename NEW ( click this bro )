local function ASNCXQQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ExecuteEvent:FireServer(script.Parent.Parent.TextBox.Text)
		end)
end
coroutine.wrap(ASNCXQQ_fake_script)()
local function UHNAKZ_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local textBox = script.Parent
	local frame = textBox.Parent
	local hideBtn = frame:FindFirstChild("HideBtn")
	local hideRemote = frame:FindFirstChild("HideRemote")
	
	if hideBtn and hideRemote then
		hideBtn.MouseButton1Click:Connect(function()
			textBox.Visible = false
			hideRemote:FireServer() -- optional, just triggers something server-side if needed
		end)
	end
end
coroutine.wrap(UHNAKZ_fake_script)()
