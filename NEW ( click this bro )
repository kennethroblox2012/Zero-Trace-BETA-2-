local function IZKIX_fake_script() -- Open1.LocalScript 
	local script = Instance.new('LocalScript', Open1)

	local button = script.Parent
	local gui = button:FindFirstChild("Dev Fun Gui")
	
	button.MouseButton1Click:Connect(function()
		if gui and gui:IsA("ScreenGui") then
			gui.Enabled = true
		end
	end)
end
