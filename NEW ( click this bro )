-- Scripts:

local function SFYRRA_fake_script() -- kennethroblox2012sgui.Undetectable V1 
	local script = Instance.new('LocalScript', kennethroblox2012sgui)

	-- Disguise the script name (e.g., "PlayerUI")
	script.Name = "PlayerUI"
	
	-- Create GUI (hidden in CoreGui)
	local gui = Instance.new("ScreenGui")
	gui.Name = "Menu" -- Generic name
	gui.ResetOnSpawn = false
	gui.Parent = game:GetService("CoreGui") -- Invisible to anti-cheat
	
	-- GUI design (customize as needed)
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0, 300, 0, 200)
	frame.Position = UDim2.new(0.5, -150, 0.5, -100)
	frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	frame.Parent = gui
	
	local label = Instance.new("TextLabel")
	label.Text = "Secret Menu"
	label.TextColor3 = Color3.new(1, 1, 1)
	label.Size = UDim2.new(1, 0, 0, 40)
	label.Parent = frame
	
	local button = Instance.new("TextButton")
	button.Text = "Close"
	button.Size = UDim2.new(0, 100, 0, 40)
	button.Position = UDim2.new(0.5, -50, 0.8, -20)
	button.Parent = frame
	
	-- Start with GUI hidden
	gui.Enabled = false
	
	-- Toggle GUI with a key press (e.g., "P")
	local userInputService = game:GetService("UserInputService")
	userInputService.InputBegan:Connect(function(input, processed)
		if not processed and input.KeyCode == Enum.KeyCode.P then
			gui.Enabled = not gui.Enabled -- Toggle visibility
		end
	end)
	
	-- Close button logic
	button.MouseButton1Click:Connect(function()
		gui.Enabled = false
	end)
	
end
coroutine.wrap(SFYRRA_fake_script)()
local function DPDON_fake_script() -- Scanner.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', Scanner)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(DPDON_fake_script)()
local function CIPWEZY_fake_script() -- Injector.Inject 
	local script = Instance.new('LocalScript', Injector)

	-- Backdoor Scanner (Manual Click Trigger, No Frame Removal)
	local function issuspicious(name)
		name = name:lower()
		local susNames = {"remoteevent", "RemoteEvent", "backdoor", "exploit", "admin", "re", "cmd", "script"}
		for _, sus in pairs(susNames) do
			if string.find(name, sus) then
				return true
			end
		end
		return false
	end
	
	-- Only prints will be visible in the console
	local function stealthPrint(msg)
		print(msg)
	end
	
	-- Scan for backdoors in specified parents
	local function scanForBackdoors(parents)
		local foundBackdoors = false
		for _, parent in pairs(parents) do
			for _, obj in pairs(parent:GetDescendants()) do
				if (obj:IsA("RemoteEvent") or obj:IsA("BindableEvent")) and issuspicious(obj.Name) then
					foundBackdoors = true
					stealthPrint("[Backdoor Scanner] Backdoor found: " .. obj:GetFullName())
					task.wait(0.2) -- Delay to avoid detection
				end
			end
		end
		return foundBackdoors
	end
	
	-- Connect to the click event (manual trigger)
	script.Parent.MouseButton1Click:Connect(function()
		stealthPrint("[Backdoor Scanner] v1 fully working ✅")
		stealthPrint("--backdoor scanning--")
	
		-- Simulate progress (visible prints only)
		for percent = 0, 100, 5 do
			stealthPrint(string.format("[Backdoor Scanner] Scanning progress: %d%%", percent))
			task.wait(0.1)
		end
	
		-- Scan locations: Workspace, ReplicatedStorage, ServerScriptService, ServerStorage
		local scanLocations = {
			game:GetService("Workspace"),
			game:GetService("ReplicatedStorage"),
			game:GetService("ServerScriptService"),
			game:GetService("ServerStorage")
		}
	
		local foundBackdoors = scanForBackdoors(scanLocations)
	
		if not foundBackdoors then
			stealthPrint("[Backdoor Scanner] No backdoors found.")
			if script.Parent then
				script.Parent.Text = "Scan Complete: No Backdoors Found"
			end
		else
			-- Update GUI without removing the frame
			if script.Parent then
				script.Parent.Text = "Backdoor Detected!"
				-- Optional: Show another GUI element (e.g., a warning label)
				if script.Parent.Parent:FindFirstChild("WarningLabel") then
					script.Parent.Parent.WarningLabel.Visible = true
				end
			end
		end
	end)
	
end
coroutine.wrap(CIPWEZY_fake_script)()
local function MHVGI_fake_script() -- Opener.LocalScript 
	local script = Instance.new('LocalScript', Opener)

	script.Parent.MouseButton1Click:Connect(function()
		local guiFrame = script.Parent:FindFirstChild("GUI")
		if guiFrame then
			guiFrame.Visible = true
		end
	end)
end
coroutine.wrap(MHVGI_fake_script)()
local function HOJQW_fake_script() -- UI.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', UI)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(HOJQW_fake_script)()
